# VastuPlan.AI Environment Variables
# Copy this file to .env and fill in your values

# Hugging Face Token (required for model access)
# Get yours at: https://huggingface.co/settings/tokens
HF_TOKEN=hf_your_token_here

# Backend URL (choose one based on your deployment)
# For Modal deployment:
VITE_API_URL=https://your-username--vastu-plan-3d-pro-fastapi-app-entry.modal.run

# For local development:
# VITE_API_URL=http://localhost:8000

# Model Cache Directory (Windows - prevents filling C: drive)
HF_HOME=D:\huggingface_cache
HUGGINGFACE_HUB_CACHE=D:\huggingface_cache\hub
TRANSFORMERS_CACHE=D:\huggingface_cache\transformers
